<?xml version="1.0"?>
<MusicFolderSyncer>
  <EnableSync />
  <ffmpegPath>C:\Program Files\ffmpeg\ffmpeg.exe</ffmpegPath>
  <Threads>2</Threads>
  <SourceDirectory>P:\Audio\Music</SourceDirectory>
  <SyncDirectory>P:\Audio\Android Music</SyncDirectory>
	<FileTypes>
		<CodecName>FLAC</CodecName>
		<CodecName>WMA</CodecName>
		<CodecName>MP3</CodecName>
		<CodecName>AAC</CodecName>
		<CodecName>OGG</CodecName>
	</FileTypes>
	<Tags>
    <Tag>
      <Name>HOME_MUSIC</Name>
      <!-- <Value>Yes</Value> -->
    </Tag>
    <Tag>
      <Name>BEST_MUSIC</Name>
    </Tag>
    <Tag>
      <Name>BEST_MUSIC_STEREO</Name>
    </Tag>
	</Tags>
	<TranscodeLosslessFiles />
	<Encoder>
    <CodecName>OGG</CodecName>
    <CodecProfile>Vorbis Q7</CodecProfile>
    <Extension>.ogg</Extension>
  </Encoder>
</MusicFolderSyncer>