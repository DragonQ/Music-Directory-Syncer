<?xml version="1.0" encoding="utf-8"?>
<MusicFolderSyncer>
  <EnableSync />
  <SourceDirectory>\\hda\Audio\Music</SourceDirectory>
  <ffmpegPath>C:\Program Files\ffmpeg\ffmpeg.exe</ffmpegPath>
  <LogLevel>Debug</LogLevel>
  <SyncSettings>
    <SyncSetting>
      <SyncDirectory>\\hda\Audio\Sync\Home Music</SyncDirectory>
      <ReplayGainMode>None</ReplayGainMode>
      <FileTypes>
        <CodecName>FLAC</CodecName>
        <CodecName>WMA</CodecName>
        <CodecName>MP3</CodecName>
        <CodecName>AAC</CodecName>
        <CodecName>OGG</CodecName>
      </FileTypes>
      <Tags>
        <Tag>
          <Name>HOME_MUSIC</Name>
          <!-- <Value>Yes</Value> -->
        </Tag>
        <Tag>
          <Name>BEST_MUSIC</Name>
        </Tag>
        <Tag>
          <Name>BEST_MUSIC_STEREO</Name>
        </Tag>
      </Tags>
      <TranscodeMode>Lossless Only</TranscodeMode>
      <Encoder>
        <CodecName>OGG</CodecName>
        <CodecProfile>Vorbis Q7</CodecProfile>
        <Extension>.ogg</Extension>
      </Encoder>
    </SyncSetting>
    <SyncSetting>
      <SyncDirectory>\\hda\Audio\Sync\Best Music</SyncDirectory>
      <ReplayGainMode>Track</ReplayGainMode>
      <FileTypes>
        <CodecName>FLAC</CodecName>
        <CodecName>WMA</CodecName>
        <CodecName>MP3</CodecName>
        <CodecName>AAC</CodecName>
        <CodecName>OGG</CodecName>
      </FileTypes>
      <Tags>
        <Tag>
          <Name>BEST_MUSIC</Name>
        </Tag>
        <Tag>
          <Name>BEST_MUSIC_STEREO</Name>
        </Tag>
      </Tags>
      <TranscodeMode>Lossless Only</TranscodeMode>
      <Encoder>
        <CodecName>AAC</CodecName>
        <CodecProfile>VBR Q1.5</CodecProfile>
        <Extension>.mp4</Extension>
      </Encoder>
    </SyncSetting>
  </SyncSettings>
</MusicFolderSyncer>