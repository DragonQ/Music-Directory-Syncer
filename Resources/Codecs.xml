<?xml version="1.0"?>
<MusicFolderSyncer>
  <Codecs>
    <Codec>
      <Name>OGG</Name>
      <Type>Lossy</Type>
      <Extensions>
        <Extension>.ogg</Extension>
        <Extension>.opus</Extension>
      </Extensions>
      <Profiles>
        <Profile>
          <Name>Vorbis Q7</Name>
          <Argument>libvorbis -aq 7</Argument>
        </Profile>
        <Profile>
          <Name>Opus 192 kb/s</Name>
          <Argument>libopus -b:a 192k</Argument>
        </Profile>
      </Profiles>
    </Codec>
    <Codec>
      <Name>FLAC</Name>
      <Type>Lossless</Type>
      <Extensions>
        <Extension>.flac</Extension>
        <Extension>.fla</Extension>
      </Extensions>
      <Profiles>
        <Profile>
          <Name>Profile 8</Name>
          <Argument>flac -compression_level 8</Argument>
        </Profile>  
      </Profiles>
    </Codec>
    <!--<Codec>
      <Name>WMA Lossless</Name>
      <Type>Lossless</Type>
      <Extensions>
        <Extension>.wma</Extension>
      </Extensions>
    </Codec>-->
    <Codec>
      <Name>WMA</Name>
      <Type>Lossy</Type>
      <Extensions>
        <Extension>.wma</Extension>
      </Extensions>
      <Profiles>
        <Profile>
          <Name>CBR 128 kb/s</Name>
          <Argument>wmav2 -b:a 128k</Argument>
        </Profile>
      </Profiles>
    </Codec>
    <Codec>
      <Name>MP3</Name>
      <Type>Lossy</Type>
      <Extensions>
        <Extension>.mp3</Extension>
      </Extensions>
      <Profiles>
        <Profile>
          <Name>VBR V0</Name>
          <Argument>V0</Argument>
        </Profile>  
        <Profile>
          <Name>VBR V2</Name>
          <Argument>V2</Argument>
        </Profile>
      </Profiles>
    </Codec>
    <Codec>
      <Name>AAC</Name>
      <Type>Lossy</Type>
      <Extensions>
        <Extension>.mp4</Extension>
        <Extension>.m4a</Extension>
        <Extension>.aac</Extension>
      </Extensions>
      <Profiles>
        <Profile>
          <Name>VBR Q1.5</Name>
          <Argument>aac -q:a 1.5</Argument>
        </Profile>
      </Profiles>
    </Codec>
  </Codecs>
</MusicFolderSyncer>